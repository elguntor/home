[alias]
	st = status
	co = checkout
	ci = commit
	br = branch
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	meld = /home/mgunter/bin/git-meld.pl
	po = push origin
	d = difftool
[user]
	name = elguntor
	email = elguntor@gmail.com
[core]
	editor = vim
	excludesfile = /Users/marshall/.gitignore_global
[color]
	ui = true
[diff]
	#external = ~/bin/git_diff.sh
[github]
	user = elguntor
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	default = simple
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
